using Gtk 4.0;
using Adw 1;

template $WdbThemes: Adw.Bin {
  ListBox list {
    valign: start;
    selection-mode: none;

    styles ["boxed-list"]

    Adw.ButtonRow import_button {
      title: _("Import");
      start-icon-name: "list-add-symbolic";
    }
  }
}

FileDialog import_dialog {
  title: _("Pick Theme");
}
