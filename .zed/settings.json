{
  "lsp": {
    "rust-analyzer": {
      "binary": {
        "path": "rust-analyzer"
      },
      "initialization_options": {
        "cargo": {
          "features": "all"
        },
        "check": {
          "command": "clippy"
        }
      }
    },
    "nil": {
      "binary": {
        "path": "nil"
      }
    },
    "nixd": {
      "binary": {
        "path": "nixd"
      }
    },
    "languages": {
      "Nix": {
        "language_servers": ["nil", "!nixd"]
      }
    }
  }
}
